{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/templates/(.*)", "dest": "/templates/$1" },
    { "src": "/admin/(.*)", "dest": "/admin/$1" },
    { "src": "/data/(.*)", "dest": "/data/$1" },

    { "src": "/(.*)", "dest": "server.js" }
  ]
}
